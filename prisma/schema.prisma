generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Post {
  id        String      @id @default(cuid())
  createdAt DateTime @default(now())
  
  content   String @db.VarChar(255)
  authorId  String

  @@index([authorId])
}